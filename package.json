{
  "name": "webcheckin",
  "version": "1.0.0",
  "description": "Web Checkin UI",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "lint": "yarn lint:all .",
    "lint:fix": "yarn lint:all --fix .",
    "lint:prefix": "yarn lint:all --fix-dry-run .",
    "lint:all": "cross-env NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.json --report-unused-disable-directives",
    "build": "cross-env NODE_ENV=production webpack --config scripts/webpack/pro.config.js --progress",
    "test:pre": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "cross-env NODE_ENV=development node ./scripts/server",
    "start:pro": "yarn build && cross-env NODE_ENV=production node ./scripts/server",
    "dll:dev": "cross-env NODE_ENV=development yarn dll",
    "dll:pro": "cross-env NODE_ENV=production yarn dll",
    "dll": "node scripts/webpack/dependencies.js",
    "precommit": "lint-staged",
    "postinstall": "yarn dll:dev"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write",
        "git add"
      ],
      "yarn.lock": [
        "git rm --cached"
      ]
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babylon",
    "semi": true,
    "arrowParens": "always",
    "tabWidth": 2
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "class-methods-use-this": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "no-plusplus": 0,
      "function-paren-newline": 0,
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/no-webpack-loader-syntax": 0,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-default-props": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "react/prop-types": 0,
      "require-yield": 0,
      "prettier/prettier": 1
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./scripts/webpack/dev.config.js"
        }
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/main.js",
      "!src/*/*/Loadable.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "<rootDir>/scripts/testing/enzymeSetup.js"
    ],
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "2.1.2",
    "autoprefixer": "8.1.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "caniuse-lite": "1.0.30000817",
    "clean-webpack-plugin": "0.1.18",
    "compression": "1.7.2",
    "cross-env": "5.1.3",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-15": "1.0.5",
    "es3ify-webpack-plugin": "0.0.1",
    "eslint": "4.19.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "eventsource-polyfill": "0.9.6",
    "export-from-ie8": "1.0.5",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "1.0.1",
    "figures": "2.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "ip": "1.1.5",
    "jest": "22.4.2",
    "json-loader": "0.5.7",
    "lint-staged": "7.0.0",
    "minimist": "1.2.0",
    "postcss": "6.0.20",
    "postcss-calc": "6.0.1",
    "postcss-color-function": "4.0.1",
    "postcss-cssnext": "3.1.0",
    "postcss-custom-media": "6.0.0",
    "postcss-custom-properties": "7.0.0",
    "postcss-custom-selectors": "4.0.1",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.3",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nesting": "^4.2.1",
    "postcss-selector-matches": "^3.0.1",
    "postcss-selector-not": "^3.0.1",
    "prettier": "1.11.1",
    "react-hot-loader": "4.0.0",
    "react-test-renderer": "15.5.4",
    "rimraf": "2.6.2",
    "shelljs": "0.8.1",
    "style-loader": "0.20.3",
    "uglifyjs-webpack-plugin": "0.4.6",
    "url-loader": "1.0.1",
    "webpack": "1.15.0",
    "webpack-dev-middleware": "1.12.2",
    "webpack-hot-middleware": "2.8.1",
    "webpack-merge": "4.1.2"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bootstrap": "3.3.7",
    "core-js": "2.5.3",
    "hoist-non-react-statics": "2.5.0",
    "invariant": "2.2.4",
    "lodash": "4.17.5",
    "prop-types": "15.6.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-loadable": "5.3.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1"
  }
}
